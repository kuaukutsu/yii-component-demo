{
  "name": "kuaukutsu/yii2-component-poc",
  "description": "Proof of concept components",
  "keywords": [
    "php",
    "proof of concept",
    "yii2",
    "component"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitriy Krivopalov",
      "email": "dimarik82@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/kuaukutsu/ds-collection/issues",
    "source": "https://github.com/kuaukutsu/ds-collection"
  },
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "ext-mongodb": "*",
    "ext-pcntl": "*",
    "enqueue/amqp-bunny": "^0.10",
    "enqueue/amqp-lib": "^0.10",
    "kuaukutsu/poc-cron": "@dev",
    "kuaukutsu/poc-saga": "^0.2",
    "kuaukutsu/poc-task": "@dev",
    "kuaukutsu/ds-collection": "^2.0",
    "kuaukutsu/ds-dto": "^2.0",
    "yiisoft/arrays": "^3.0",
    "yiisoft/strings": "^2.1",
    "yiisoft/validator": "^1.0",
    "yiisoft/yii2": "^2.0.49",
    "yiisoft/yii2-queue": "^2.3",
    "yiisoft/yii2-mongodb": "^3.0",
    "yiisoft/yii2-redis": "^2.0"
  },
  "require-dev": {
    "ext-apcu": "*",
    "ext-sync": "*",
    "vimeo/psalm": "^5.15",
    "friendsofphp/php-cs-fixer": "^3.13",
    "icanhazstring/composer-unused": "^0.8",
    "phpunit/phpunit": "^10.4",
    "rector/rector": "^0.18",
    "roave/infection-static-analysis-plugin": "^1.3",
    "roave/security-advisories": "dev-latest",
    "slevomat/coding-standard": "^8.7",
    "squizlabs/php_codesniffer": "^3.7",
    "yiisoft/yii2-debug": "^2.1.19",
    "yiisoft/yii2-faker": "^2.0.0",
    "yiisoft/yii2-gii": "^2.1.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "vcs",
      "url": "git@github.com:kuaukutsu/poc-cron.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:kuaukutsu/poc-saga.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:kuaukutsu/poc-task.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:kuaukutsu/ds-collection.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:kuaukutsu/ds-dto.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "kuaukutsu\\poc\\demo\\": ""
    },
    "files": [
      "functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "kuaukutsu\\poc\\demo\\tests\\": "tests"
    }
  },
  "config": {
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "infection/extension-installer": true,
      "yiisoft/yii2-composer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
